#include<stdio.h>
int main()
   {
   	char lowercase_letter,uppercase_letter,consonent,c,symbol;
   	printf("enter a symbol");
   	scanf("%c",&symbol);
   	lowercase_letter=(c=='a'||c=='e'||c=='i'||c=='o'||c=='u');
   	uppercase_letter=(c=='A'||c=='E'||c=='I'||c=='o'||c=='U');
   	if(lowercase_letter||uppercase_letter)
   	printf("%c is a vowel",symbol);
   	else
    printf("%c is a consonant",symbol);
	return 0;    
   	
   }
